:8080

handle_errors 502 {
	root * /static/errors
	rewrite * /502.html
	file_server
}

@websockets {
	header Connection *Upgrade*
	header Upgrade websocket
}

reverse_proxy @websockets :9664 {
	header_up Origin {$LILA_URL}
}

reverse_proxy :9663 {
	header_up Host {$LILA_DOMAIN}
	header_up Origin {$LILA_URL}
}
